{
    "root": true,
    "env":
    {
        "browser": true,
        "node": true
    },
    "extends": ["standard", "plugin:vue/strongly-recommended", "plugin:vue/essential"],
    "rules":
    {
        "indent": ["error", 4],
        "space-before-function-paren": ["error", "never"],
        "no-unused-expressions": "off"
    },
    "plugins": [
        "vue"
    ]
}
